<?xml version="1.0" encoding="UTF-8"?><window type="dialog">	<defaultcontrol>104</defaultcontrol>	<coordinates>		<left>0</left>		<top>0</top>	</coordinates>	<controls>		<!-- Background -->		<control type="image">			<texture>Background/white.png</texture>			<colordiffuse>FF181818</colordiffuse>		</control>		<control type="image">			<width>200</width>			<height>640</height>			<top>430</top>			<left>130</left>			<texture>logo_l.png</texture>			<colordiffuse>FFff0000</colordiffuse>			<animation effect="fade" start="100" end="40" time="4900" pulse="true" condition="true">Conditional</animation>		</control>		<control type="image">			<width>100</width>			<height>640</height>			<top>430</top>			<left>20</left>			<texture>logo.png</texture>			<colordiffuse>white</colordiffuse>		</control>		<control type="image">			<width>3</width>			<height>1080</height>			<top>80</top>			<left>330</left>			<texture>divider.png</texture>			<colordiffuse>FFa6a6a6</colordiffuse>		</control>		<control type="group">			<!-- Header -->			<control type="group">				<control type="image">					<width>1980</width>					<height>83</height>					<left>-17</left>					<top>-1</top>					<texture>Button/button-focus_grey.png</texture>					<colordiffuse>$INFO[Window().Property(texture.color1)]</colordiffuse>				</control>				<control type="image">					<top>-3.3</top>					<left>1835</left>					<width>86</width>					<height>86</height>					<texture>overlaywatched_1.png</texture>					<colordiffuse>$INFO[Window().Property(texture.color1)]</colordiffuse>				</control>				<control type="image">					<top>-3.3</top>					<left>-3.3</left>					<width>86</width>					<height>86</height>					<texture flipx="true">overlaywatched_1.png</texture>					<colordiffuse>$INFO[Window().Property(texture.color1)]</colordiffuse>				</control>				<!-- Label -->				<control type="label" id="101">					<width>1920</width>					<height>80</height>					<font>font15</font>					<align>center</align>					<aligny>center</aligny>					<label>[B][COLOR $INFO[Window().Property(texture.color2)]]$INFO[Window().Property(message.title)]: $INFO[Window().Property(message.logfile)][/COLOR][/B]</label>				</control>			</control>			<!-- Side Buttons -->			<control type="grouplist" id="104">				<onright>103</onright>				<top>96</top>				<left>16</left>				<width>400</width>				<height>952</height>				<itemgap>6</itemgap>				<defaultcontrol>201</defaultcontrol>				<control type="button" id="201">					<align>left</align>					<height>60</height>					<width>300</width>					<textcolor>FFd22d2d</textcolor>					<focusedcolor>FF19e64d</focusedcolor>					<texturefocus colordiffuse="">Button/green.png</texturefocus>					<texturenofocus colordiffuse="">Button/red.png</texturenofocus>					<label>Upload Log</label>				</control>				<control type="button" id="202">					<align>left</align>					<height>60</height>					<width>300</width>					<textcolor>FFd22d2d</textcolor>					<focusedcolor>FF19e64d</focusedcolor>					<texturefocus colordiffuse="">Button/green.png</texturefocus>					<texturenofocus colordiffuse="">Button/red.png</texturenofocus>					<label>Kodi Log</label>					<visible>!String.IsEqual(Window().Property(message.buttons), true)</visible>				</control>				<control type="button" id="203">					<align>left</align>					<height>60</height>					<width>300</width>					<textcolor>FFd22d2d</textcolor>					<focusedcolor>FF19e64d</focusedcolor>					<texturefocus colordiffuse="">Button/green.png</texturefocus>					<texturenofocus colordiffuse="">Button/red.png</texturenofocus>					<label>Old Log</label>					<visible>!String.IsEqual(Window().Property(message.buttons), true)</visible>				</control>				<control type="button" id="204">					<align>left</align>					<height>60</height>					<width>300</width>					<textcolor>FFd22d2d</textcolor>					<focusedcolor>FF19e64d</focusedcolor>					<texturefocus colordiffuse="">Button/green.png</texturefocus>					<texturenofocus colordiffuse="">Button/red.png</texturenofocus>					<label>Wizard Log</label>					<visible>!String.IsEqual(Window().Property(message.buttons), true)</visible>				</control>				<control type="button" id="205">					<align>left</align>					<height>60</height>					<width>300</width>					<textcolor>FFd22d2d</textcolor>					<focusedcolor>FF19e64d</focusedcolor>					<texturefocus colordiffuse="">Button/green.png</texturefocus>					<texturenofocus colordiffuse="">Button/red.png</texturenofocus>					<label>Close</label>				</control>			</control>			<!-- Text Box -->			<control type="textbox" id="102">				<left>350</left>				<top>96</top>				<width>1500</width>				<height>952</height>				<align>justify</align>				<font>font13</font>				<pagecontrol>103</pagecontrol>				<autoscroll delay="3000" time="2000" repeat="10000">!Control.HasFocus(103)</autoscroll>				<label>[COLOR white]$INFO[Window().Property(message.logmsg)][/COLOR]</label>			</control>			<!-- Scrollbar -->			<control type="scrollbar" id="103">				<left>1872</left>				<top>96</top>				<width>32</width>				<height>952</height>				<onleft>104</onleft>				<onup>102</onup>				<ondown>102</ondown>				<texturesliderbackground colordiffuse="$INFO[Window().Property(texture.color2)]">Button/scrollbar-V-background.png</texturesliderbackground>				<texturesliderbar colordiffuse="$INFO[Window().Property(texture.color2)]">Button/scrollbar-V.png</texturesliderbar>				<texturesliderbarfocus colordiffuse="$INFO[Window().Property(texture.color1)]">Button/scrollbar-V-focus.png</texturesliderbarfocus>				<subtype>text</subtype>			</control>		</control>	</controls></window>