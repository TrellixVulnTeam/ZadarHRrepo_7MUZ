<?xml version="1.0" encoding="UTF-8"?><window type="dialog">	<defaultcontrol>104</defaultcontrol>	<coordinates>		<left>140</left>		<top>110</top>	</coordinates>	<controls>		<!-- Background -->		<control type="image">			<posx>-140</posx>			<posy>-650</posy>			<width>1920</width>			<height>1680</height>			<texture border="2" colordiffuse="black">Background/shadow_mask_v.png</texture>		</control>		<control type="image">			<width>1650</width>			<height>700</height>			<texture border="2" colordiffuse="FF997300">Background/ContentPanel.png</texture>		</control>		<control type="image">			<left>45</left>			<top>30</top>			<width>1590</width>			<height>650</height>			<texture border="2" diffuse="Background/details_back_white.png" colordiffuse="8A997300">Background/wall.jpg</texture>		</control>		<control type="group">			<!-- Header -->			<control type="group">				<width>1190</width>				<height>80</height>				<control type="label" id="101">					<top>20</top>					<left>580</left>					<label>[COLOR $INFO[Window().Property(texture.color2)]]$INFO[Window().Property(message.title)][/COLOR]</label>				</control>			</control>			<!-- Side Buttons -->			<control type="group" id="104">				<onright>103</onright>				<top>630</top>				<left>680</left>				<width>240</width>				<height>252</height>				<itemgap>16</itemgap>				<defaultcontrol>201</defaultcontrol>				<control type="button" id="201">					<align>center</align>					<height>50</height>					<width>330</width>					<textcolor>FFd22d2d</textcolor>					<focusedcolor>FF19e64d</focusedcolor>					<texturefocus>Button/ok_button_green.png</texturefocus>					<texturenofocus>Button/ok_button_red.png</texturenofocus>					<label>Close</label>					<onup>103</onup>					<ondown>103</ondown>					<onleft>103</onleft>					<onright>103</onright>				</control>			</control>			<!-- Text Box -->			<control type="textbox" id="102">				<left>40</left>				<top>111</top>				<width>1600</width>				<height>952</height>				<label>[COLOR white]$INFO[Window().Property(message.msg)][/COLOR]</label>			</control>			<!-- Scrollbar -->			<control type="scrollbar" id="103">				<left>1872</left>				<top>96</top>				<width>32</width>				<height>952</height>				<onup>104</onup>				<ondown>104</ondown>				<onleft>104</onleft>				<onright>104</onright>				<texturesliderbackground/>				<texturesliderbar/>				<texturesliderbarfocus/>				<subtype>text</subtype>			</control>		</control>	</controls></window>